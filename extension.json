{
	"name": "JsCalendar",
	"version": "0.3.0-alpha",
	"author": "Steffen Beyer and others",
	"url": "https://github.com/improper/mediawiki-extensions-yasec",
	"description": "Yet Another Simple Event Calendar",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.34.0"
	},
	"ResourceModules": {
		"ext.yasec": {
			"scripts": [
				"fullcalendar/lib/moment.min.js",
				"fullcalendar/fullcalendar/fullcalendar.min.js",
				"ext.yasec.core.js"
			],
			"styles": [
				"fullcalendar/fullcalendar/fullcalendar.css",
				"ext.yasec.css"
			],
			"dependencies": [
				"jquery.ui.datepicker"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "yasec/resources"
	},
	"AutoloadClasses": {
		"MediaWiki\\JsCalendar\\EventCalendar": "includes/EventCalendar.php"
	},
	"Hooks": {
		"ParserFirstCallInit": "MediaWiki\\JsCalendar\\EventCalendar::onParserFirstCallInit"
	},
	"config": {
		"ECMaxCacheTime": {
			"value": 86400
		}
	},
	"manifest_version": 2
}
